<%= render :partial => 'admin/shared/outfit_tabs', :locals => {:current => "Products"} %>

<%= form_for(:outfit_product, :url => admin_outfit_outfit_products_url(@outfit)) do |form| %>
  <table class="basic-table">
    <%= render :partial => "form", :locals => { :f => form } %>
  </table>
  <p class="form-buttons">
    <%= button t("update") %>
    <%= t('or') %> <%= link_to t("cancel"), "#", :id => "cancel_link" %>
  </p>
<% end %>